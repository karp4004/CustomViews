<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.okarpov.customviews.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        android:id="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <com.example.okarpov.customviews.CustomList
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/cList"
        android:layout_alignParentTop="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:id="@+id/imageView"
        android:src="@android:drawable/stat_sys_phone_call_forward"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:background="@android:drawable/stat_sys_phone_call_forward" />
</RelativeLayout>
